{
    "name": "gpupo/common-sdk",
    "description": "Componente de uso comum entre SDKs para integração a partir de aplicações PHP com Restful webservices",
    "homepage": "https://opensource.gpupo.com/common-sdk/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["sdk"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gpupo\\CommonSdk\\": "src",
            "Gpupo\\Tests\\CommonSdk\\": "tests"
        }
    },
    "require": {
        "php": "^7.1.3",
        "symfony/console": "^4.0",
        "psr/log": "^1.0",
        "gpupo/cache": "^1.3",
        "gpupo/common": "^2.4",
        "twig/twig": "^2.0",
        "monolog/monolog": "^1.11",
        "codeclimate/php-test-reporter": "@dev",
        "symfony/var-dumper": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "bin": ["bin/common-sdk"],
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
